<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.foi.air.storknest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.MainScreenActivity" android:label="@string/title_activity_main_screen" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".app.feed.NewFeedActivity" android:label="Adding New Feed" />
        <activity android:name=".app.diaper.NewDiaperActivity" android:label="Adding New Diaper" />
        <activity android:name=".app.diaper.DiaperListActivity" android:label="List of Diapers" />
        <activity android:name=".app.feed.FeedListActivity" android:label="List Of Feeds" ></activity>
        <activity android:name=".app.hygiene.NewHygieneActivity" android:label="Adding New Hygiene" ></activity>
        <activity android:name=".app.hygiene.HygieneListActivity" android:label="List Of Hygienes" ></activity>
        <activity android:name=".app.measure.NewMeasureActivity" android:label="Adding New Measure" ></activity>
        <activity android:name=".app.measure.MeasureListActivity" android:label="List Of Measures" ></activity>
        <activity android:name=".app.doctor.NewDoctorActivity" android:label="Adding New Doctor" ></activity>
        <activity android:name=".app.doctor.DoctorListActivity" android:label="List Of Doctors" ></activity>
        <activity android:name=".app.vaccination.NewVaccinationActivity" android:label="Adding New Vaccination" ></activity>
        <activity android:name=".app.vaccination.VaccinationListActivity" android:label="List Of Vaccinations" ></activity>
    </application>

</manifest>