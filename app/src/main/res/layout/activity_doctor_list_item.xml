<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".app.doctor.DoctorListActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/created_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="2dp"
            android:text="Time"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#4d4d4d"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="230dp"
            tools:layout_editor_absoluteY="154dp" />

    <LinearLayout
        android:layout_width="344dp"
        android:layout_height="125dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="2dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="344dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="2dp"
            tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name:"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#4d4d4d"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp"/>

        <TextView
            android:id="@+id/doctor_nameSurname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:text="Name"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="344dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="2dp"
            tools:ignore="MissingConstraints">

        <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Specialization:"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#4d4d4d"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="133dp"/>

        <TextView
            android:id="@+id/doctor_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:text="Type"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="344dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="2dp"
            tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Working hours:"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#4d4d4d"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp"/>

        <LinearLayout
            android:layout_width="344dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="2dp"
            tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/doctor_workingHoursFrom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:text="Working from"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="-"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#4d4d4d"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp"/>

        <TextView
            android:id="@+id/doctor_workingHoursTo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:text="Working to"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp" />

        </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="344dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="2dp"
            tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Telephone"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#4d4d4d"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp"/>

        <TextView
            android:id="@+id/doctor_telephone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:text="Telephone"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="133dp" />

        </LinearLayout>

    </LinearLayout>
    </LinearLayout>



</android.support.constraint.ConstraintLayout>